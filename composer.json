{
    "name": "kikwik/gmap-bundle",
    "description": "Google Map and Geocoder support for Symfony 5",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Davide Scarpato",
            "email": "davide@kikwik.it"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "ext-json": "*",
        "symfony/framework-bundle": "^5.3|^6.0",
        "geocoder-php/google-maps-provider": "^4.5",
        "symfony/http-client": "^5.3",
        "willdurand/geocoder-bundle": "^5.13",
        "nyholm/psr7": "^1.5",
        "symfony/yaml": "^5.3",
        "doctrine/orm": "^2.7",
        "stof/doctrine-extensions-bundle": "^1.3",
        "twig/extra-bundle": "^2.12|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Kikwik\\GmapBundle\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
